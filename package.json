{
  "name": "@feedbackfarm/core",
  "version": "1.0.9",
  "description": "This package allow you to send feedback to the feedback.farm application.",
  "main": "lib/FeedbackCore.js",
  "types": "lib/FeedbackCore.d.ts",
  "homepage": "https://feedback.farm",
  "bugs": {
    "url": "https://github.com/feedbackfarm/core/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/feedbackfarm/core.git"
  },
  "scripts": {
